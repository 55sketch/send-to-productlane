{
  "name": "Send To Productlane",
  "description": "Sends highlighted text and URL of page to Productlane as a note",
  "version": "1",
  "manifest_version": 3,
  "permissions": ["contextMenus", "storage"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "productlane_logo.png",
    "48": "productlane_logo.png",
    "128": "productlane_logo.png"
  }
}
